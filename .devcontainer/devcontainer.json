{
    "dockerComposeFile": [
      "compose.yml"
    ],
    "service": "async-graphql",
    "workspaceFolder": "/workspace",
    "customizations": {
      "vscode": {
        "extensions": [
          "GitHub.copilot",
          "ms-azuretools.vscode-docker",
          "rust-lang.rust-analyzer",
          "tamasfe.even-better-toml"
        ],
        "settings": {
          "files.watcherExclude": {
            "**/target/**": true
          }
        }
      }
    }
  }
  